                                        <select class="nice-select">
                                            <option>Choose Area</option>
                                            <option>Asokoro</option>
                                            <option>Maitama</option>
                                            <option>Katampe</option>
                                            <option>Guzappe</option>
                                            <option>Katampe extension</option>
                                            <option>Jabi</option>
                                            <option>Wuye</option>
                                        </select>
                                    </div>
