@{
    Layout = "/Pages/Shared/_Layout.cshtml";
}

<div class="ltn__breadcrumb-area text-left bg-overlay-white-30 bg-image " data-bs-bg="/themes/quarter/img/bg/14.jpg" style="background-image: url(/themes/quarter/img/bg/14.jpg);">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="ltn__breadcrumb-inner">
                    <h1 class="page-title">@ViewBag.Title</h1>
                    <div class="ltn__breadcrumb-list">
                        <ul>
                            <li><a href="/"><span class="ltn__secondary-color"><i class="fas fa-home"></i></span> Home Page</a></li>

                            <li>@ViewBag.Title</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@RenderBody()


